@page "/test"

<h3>Updated Test Page</h3>

<p>This is an updated test page with some random changes.</p>

<button class="btn btn-primary" @onclick="IncrementCount">Click me if you dare!</button>

<p>Current count: @currentCount</p>

@code {
    private int currentCount = 0;

    private void IncrementCount()
    {
        currentCount += 2;
    }
}
